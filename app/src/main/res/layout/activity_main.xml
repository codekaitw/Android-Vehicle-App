<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity" android:scrollbarSize="10dp" android:focusableInTouchMode="true">
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Hello World!"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="0.023"
            android:id="@+id/textView"
            android:textSize="30sp" android:selectAllOnFocus="false" app:layout_constraintHorizontal_bias="0.497"/>
    <Button
            android:text="add"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/btn_GoAllVehicleView"
            android:layout_marginBottom="530dp" app:layout_constraintBottom_toTopOf="@+id/btn_GoAvailableVehicleView"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="60dp"/>
    <Button
            android:text="ailablevA"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/btn_GoAvailableVehicleView"
            app:layout_constraintEnd_toStartOf="@+id/btn_GoSoldVehicleView"
            android:layout_marginBottom="33dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="6dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginEnd="16dp"
    />
    <Button
            android:text="sluo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/btn_GoSoldVehicleView"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/btn_GoAvailableVehicleView" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="33dp"/>
    <Spinner
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/spinner"
            android:layout_marginTop="16dp"
            android:layout_marginStart="18dp" app:layout_constraintTop_toBottomOf="@+id/textView"
            app:layout_constraintStart_toEndOf="@+id/textView" android:minHeight="48dp"/>
    <ListView
            android:id="@+id/listView_AllVehicle"
            android:layout_width="match_parent"
            android:layout_height="match_parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="130dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="110dp"
            android:scrollbarAlwaysDrawVerticalTrack="false" android:scrollbars="horizontal|vertical"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="1.0"/>

</androidx.constraintlayout.widget.ConstraintLayout>